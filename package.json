{
  "name": "@iyulab/components",
  "description": "web-components library based on lit-element made by iyulab",
  "version": "0.3.0",
  "keywords": [
    "iyulab",
    "components",
    "web-components",
    "lit",
    "lit-element"
  ],
  "license": "MIT",
  "author": "iyulab",
  "repository": {
    "type": "git",
    "url": "https://github.com/iyulab/node-components.git"
  },
  "files": [
    "dist",
    "LICENSE",
    "README.md",
    "CHANGELOG.md",
    "package.json"
  ],
  "type": "module",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./src/index.d.ts",
      "import": "./src/index.js"
    },
    "./dist/*": {
      "types": "./src/*.d.ts",
      "import": "./src/*"
    }
  },
  "scripts": {
    "test": "vite --force",
    "build": "eslint && vite build"
  },
  "dependencies": {
    "@floating-ui/dom": "^1.7.5",
    "lit": "^3.3.2",
    "reflect-metadata": "^0.2.2"
  },
  "peerDependencies": {
    "@lit/react": ">=1.0.0",
    "react": ">=18.0.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    },
    "@lit/react": {
      "optional": true
    }
  },
  "devDependencies": {
    "@eslint/js": "^10.0.1",
    "@types/node": "^25.3.0",
    "eslint": "^10.0.2",
    "eslint-plugin-lit": "^2.2.1",
    "globals": "^17.3.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.56.1",
    "vite": "^7.3.1",
    "vite-plugin-dts": "^4.5.4",
    "vite-plugin-static-copy": "^3.2.0"
  }
}
