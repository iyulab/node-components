{
  "name": "@iyulab/components",
  "description": "web-components library by iyulab based on lit-element",
  "version": "0.1.4",
  "keywords": [
    "iyulab",
    "web-components",
    "lit-element"
  ],
  "license": "MIT",
  "author": "iyulab",
  "repository": {
    "type": "git",
    "url": "https://github.com/iyulab/node-components.git"
  },
  "files": [
    "dist",
    "LICENSE",
    "README.md",
    "CHANGELOG.md",
    "package.json"
  ],
  "type": "module",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./src/index.d.ts",
      "import": "./src/index.js"
    },
    "./dist/*": {
      "types": "./src/*.d.ts",
      "import": "./src/*"
    }
  },
  "scripts": {
    "start": "vite --force",
    "build": "eslint && vite build"
  },
  "dependencies": {
    "@floating-ui/dom": "^1.7.4",
    "lit": "^3.3.1",
    "react": "^19.2.1",
    "reflect-metadata": "^0.2.2"
  },
  "devDependencies": {
    "@lit/react": "^1.0.8",
    "@eslint/js": "^9.39.1",
    "@types/node": "^24.10.1",
    "eslint": "^9.39.1",
    "eslint-plugin-lit": "^2.1.1",
    "globals": "^16.5.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.48.1",
    "vite": "^7.2.6",
    "vite-plugin-dts": "^4.5.4",
    "vite-plugin-static-copy": "^3.1.4"
  }
}